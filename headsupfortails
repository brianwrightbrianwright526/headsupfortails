</script>
    <script>
      function scrollToTop() {
            $(window).scrollTop(0);
        }
 $(window).scroll(function() { 
  var scroll = $(window).scrollTop();
 
  if (scroll >= 70) {
  $(".bb_head").addClass("fixedheaders");
  //$('#progressBar').addClass("fixedprogress");
     $(".scrolltops").addClass("scrollbtm");
  } else {
  $(".bb_head").removeClass("fixedheaders");
     $(".scrolltops").removeClass("scrollbtm");
  }
 });
function menufunction() {
  var x = document.getElementById("myTopnav");
  if (x.className === "bb_head") {
    x.className += " responsive";
  } else {
    x.className = "bb_head";
    
  }
}

$(document).ready(function(){
    $('.mobileicon').click(function(){
 $(window).scrollTop(0);
        $('.bb_head').addClass('responsive');
        $('.overlay').addClass('fixoverlay');
        $('body').addClass('overflow');

    })
    $('.crossicon').click(function(){

$('.bb_head').removeClass('responsive');
$('.overlay').removeClass('fixoverlay');
$('body').removeClass('overflow');
});
$('.overlay').click(function(){

$('.bb_head').removeClass('responsive');
$(this).removeClass('fixoverlay');
$('body').removeClass('overflow');
});
$(".dropdown").click(function(e) {
    if ($(e.target).is(".dropdown > a")){
    $(this).children('.sub_menu').toggleClass('submenusho');
    }
  $('.subdropdown').children('.submenusho').toggle();
  });
$(".subdropdown").click(function(e) {
    if ($(e.target).is(".subdropdown > a"))
    $(this).children('.sub_menu_child').toggleClass('submenusho');
  //$(this).children('.sub_menu_child').toggleClass('submenusho');
   $(this).children('.submenusho').toggle();
  });
});
